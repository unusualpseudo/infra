{
  "packageRules": [
    {
      "matchUpdateTypes": [
        "major"
      ],
      "labels": [
        "type/major"
      ]
    },
    {
      "matchUpdateTypes": [
        "minor"
      ],
      "labels": [
        "type/minor"
      ]
    },
    {
      "matchUpdateTypes": [
        "patch"
      ],
      "labels": [
        "type/patch"
      ]
    },
    {
      "matchDatasources": [
        "terraform-provider"
      ],
      "addLabels": [
        "renovate/terraform"
      ]
    },
    {
      "matchManagers": [
        "github-actions"
      ],
      "addLabels": [
        "renovate/github-action"
      ]
    },
    {
      "matchDatasources": [
        "galaxy",
        "galaxy-collection"
      ],
      "addLabels": [
        "renovate/ansible"
      ]
    }
  ]
}
