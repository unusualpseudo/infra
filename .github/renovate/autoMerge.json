{
  "packagesRules": [
    {
      "matchManagers": [
        "github-actions"
      ],
      "automerge": true,
      "automergeType": "branch",
      "requiredStatusChecks": null,
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    },
    {
      "description": "Auto Merge Pre-Commit",
      "matchManagers": [
        "pre-commit"
      ],
      "automerge": true,
      "automergeType": "branch",
      "requiredStatusChecks": null,
      "matchUpdateTypes": [
        "minor",
        "patch"
      ]
    }
  ]
}
